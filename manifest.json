{
  "name": "Synch",
  "description": "Text To Speech for the visually impaired",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tts",
    "activeTab",
    "unlimitedStorage"
  ],

  "icons":{
    "16": "imgs/logo.png",
    "24": "imgs/logo.png",
    "32": "imgs/logo.png"

  },

  "action":{

     "default_title":"Synch Settings",
     "default_popup": "popup.html"


   },

  "content_scripts": [
   {
     "matches": ["<all_urls>"],

     "js": ["app.js"]
   }
 ]

}
