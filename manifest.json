{
  "name": "Synch",
  "description": "Text To Speech for the visually impaired",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tts",
    "activeTab",
    "tabs",
    "webNavigation",
    "unlimitedStorage",
    "contextMenus",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],

  "declarative_net_request":{
    "id" : 1,
  "priority": 1,
  "action" : { "type" : "block" },
  "condition" : {
    "urlFilter" : "easy_list.json",
    "domains" : ["easy_list.json"],
    "resourceTypes" : ["script"]


  },



  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background":{
    "service_worker": "background.js",
    "service_worker":"contextMenu.js"


  },
  "icons":{
    "16": "imgs/logo.png",
    "24": "imgs/logo.png",
    "32": "imgs/logo.png"

  },

  "action":{

     "default_title":"Synch Settings",
     "default_popup": "popup.html"


   },

  "content_scripts": [
   {
     "matches": ["<all_urls>"],

     "js": ["app.js",
       "contextMenu.js",
       "adblock.js"
     ]
   }
 ]

}
